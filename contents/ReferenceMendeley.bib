issn = {0894-8755},
journal = {Journal of Climate},
keywords = {Australian rainfall,El Nino–Southern Oscillation/Indian Ocean Dipole (,Global hydrological cycle,Moisture transport,Sea surface salinity (SSS)},
mendeley-groups = {Diss2},
month = {aug},
number = {15},
pages = {6707--6730},
publisher = {journals.ametsoc.org},
title = {{Near-Surface Salinity Reveals the Oceanic Sources of Moisture for Australian Precipitation through Atmospheric Moisture Transport}},
url = {https://journals.ametsoc.org/view/journals/clim/33/15/jcliD190579.xml http://journals.ametsoc.org/doi/10.1175/JCLI-D-19-0579.1},
