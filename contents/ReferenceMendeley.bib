journal = {International Journal of Remote Sensing},
keywords = {Indian Ocean Dipole (IOD) events,Remote sensing,Sea surface temperature anomaly (SSTA),Southeastern Tropical Indian Ocean (SETIO),Surface chlorophyll-a},
mendeley-groups = {Diss4},
month = {jan},
number = {1},
pages = {171--184},
publisher = {Taylor &Francis},
title = {{Surface chlorophyll-a variations in the Southeastern Tropical Indian Ocean during various types of the positive Indian Ocean Dipole events}},
url = {https://www.tandfonline.com/doi/abs/10.1080/01431161.2019.1637962 https://www.tandfonline.com/doi/full/10.1080/01431161.2019.1637962},
