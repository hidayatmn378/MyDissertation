pages = {GJ22007},
publisher = {jstage.jst.go.jp},
title = {{Unique behavior of marine conditions in the Java Sea reconstructed from a 70 yr coral $\delta$18O and Sr/Ca record from the Seribu Islands, Indonesia}},
