publisher = {Elsevier},
title = {{The impact of trends in South Asia Low pressure center on North Indian Ocean air-sea heat fluxes}},
url = {https://www.sciencedirect.com/science/article/pii/S1364682621002364 https://linkinghub.elsevier.com/retrieve/pii/S1364682621002364},
volume = {226},
