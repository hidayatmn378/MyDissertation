issn = {2169897X},
journal = {Journal of Geophysical Research: Atmospheres},
keywords = {IOD,Indonesian fires,Walker circulation,drought,fire forecast,the type of El Nino},
mendeley-groups = {Dissertation},
month = {aug},
number = {15},
pages = {7974--7988},
publisher = {John Wiley & Sons, Ltd},
title = {{Connecting Indonesian Fires and Drought With the Type of El Nino and Phase of the Indian Ocean Dipole During 1979-2016}},
