pages = {1367--1383},
publisher = {Springer},
title = {{Indian Ocean tripole mode and its associated atmospheric and oceanic processes}},
url = {https://link.springer.com/article/10.1007/s00382-020-05331-1 https://link.springer.com/10.1007/s00382-020-05331-1},
