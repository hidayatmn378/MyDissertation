journal = {International Journal of Climatology},
keywords = {1,Bay of Bengal,El-Nino and La-Nina,cyclone heat potential,oceanic heat content,upper ocean heat content},
mendeley-groups = {Diss5},
month = {dec},
number = {15},
pages = {5619--5632},
publisher = {Wiley Online Library},
