doi = {10.1016/j.physa.2019.122138},
file = {:home/ocean/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Rafiq, Mustafa, Farooq - 2019 - Numerical assessment of B{\"{o}}dewadt flow and heat transfer over a permeable disk with variable fluid pr(2).pdf:pdf},
issn = {03784371},
journal = {Physica A: Statistical Mechanics and its Applications},
keywords = {B{\"{o}}dewadt flow,Heat transfer,Similarity solution,Variable viscosity,Wall suction},
month = {nov},
pages = {122138},
publisher = {North-Holland},
title = {{Numerical assessment of B{\"{o}}dewadt flow and heat transfer over a permeable disk with variable fluid properties}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0378437119312427},
volume = {534},
year = {2019}
}

