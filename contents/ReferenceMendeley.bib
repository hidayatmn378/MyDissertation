journal = {International Journal of Remote Sensing},
keywords = {Australian-Indonesian Monsoon,El Nino-Southern Oscillation,Ocean surface chlorophyll-a,Sea surface temperature,Sea surface wind stress},
mendeley-groups = {Diss4},
